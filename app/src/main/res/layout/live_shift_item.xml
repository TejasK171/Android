<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <LinearLayout
        android:id="@+id/ll_moring"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        android:weightSum="6">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/img_productivity_status"
            android:layout_width="0dp"
            android:layout_height="@dimen/_15sdp"
            android:layout_weight=".3"
            android:layout_margin="@dimen/_5sdp"
            android:background="@drawable/green_rounded"/>
        <com.jio.jioinfra.custom.TextViewMedium
            android:id="@+id/txt_resource"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_weight="2.2"
            app:layout_constraintTop_toBottomOf="@+id/txt_date"
            app:layout_constraintLeft_toLeftOf="parent"
            android:gravity="left"
            android:layout_gravity="center|center_vertical"
            android:padding="@dimen/_5sdp"
            android:textSize="@dimen/_8sdp"
            android:textColor="@color/txt_grey_color"
            />
        <com.jio.jioinfra.custom.TextViewMedium
            android:id="@+id/txt_citrix_session_status"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_weight="1"
            android:layout_gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/txt_date"
            app:layout_constraintLeft_toLeftOf="parent"
            android:gravity="center"
            android:padding="@dimen/_5sdp"
            android:textSize="@dimen/_8sdp"
            android:textColor="@color/acknowladge_back"
            />
        <com.jio.jioinfra.custom.TextViewMedium
            android:id="@+id/txt_ngo_session_status"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_weight="1"
            app:layout_constraintTop_toBottomOf="@+id/txt_date"
            app:layout_constraintLeft_toLeftOf="parent"
            android:gravity="center"
            android:layout_gravity="center"
            android:padding="@dimen/_5sdp"
            android:textSize="@dimen/_8sdp"
            android:textColor="@color/acknowladge_back"
            />
        <com.jio.jioinfra.custom.TextViewMedium
            android:id="@+id/txt_alerts_acknw_count"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text=""
            android:layout_weight="0.6"
            app:layout_constraintTop_toBottomOf="@+id/txt_date"
            app:layout_constraintLeft_toLeftOf="parent"
            android:gravity="center"
            android:layout_gravity="center"
            android:padding="@dimen/_5sdp"
            android:textSize="@dimen/_8sdp"
            android:textColor="@color/txt_grey_color"
            />
        <com.jio.jioinfra.custom.TextViewMedium
            android:id="@+id/txt_details"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Details"
            android:layout_weight="0.8"
            android:layout_gravity="center"
            app:layout_constraintTop_toBottomOf="@+id/txt_date"
            app:layout_constraintLeft_toLeftOf="parent"
            android:gravity="center"
            android:paddingTop="@dimen/_5sdp"
            android:paddingBottom="@dimen/_5sdp"
            android:textSize="@dimen/_8sdp"
            android:textColor="@color/txt_grey_color"
            />
    </LinearLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/const_show"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintTop_toBottomOf="@+id/ll_moring"
        >
        <View
            android:id="@+id/view_id"
            android:layout_width="match_parent"
            android:background="@color/black"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_height="1dp"/>
        <LinearLayout
            android:id="@+id/llll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/view_id"
            android:weightSum="2">


            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_citrix"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Citrix"
                android:layout_weight="1"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_10sdp"
                android:textColor="@color/black"
                />
            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_ngo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="NGO"
                android:layout_weight="1"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_10sdp"
                android:textColor="@color/black"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/llll"
            android:weightSum="4">

            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_alart"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Start"
                android:layout_weight="1"
                android:layout_gravity="center"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/black"
                />
            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_avg"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="End"
                android:layout_weight="1"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/black"
                />
            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_delead"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="Start"
                android:layout_weight="1"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/black"
                />
            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_insiteate"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text="End"
                android:layout_weight="1"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/black"
                />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/lll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintTop_toBottomOf="@+id/ll"
            android:weightSum="4">

            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_start_time_citrix"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=""
                android:layout_weight="1"
                android:layout_gravity="center"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/jioinfra_gray"
                />
            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_end_time_citrix"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=""
                android:layout_weight="1"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:layout_gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/jioinfra_gray"
                />
            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_start_time_ngo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=""
                android:layout_weight="1"
                app:layout_constraintTop_toBottomOf="@+id/txt_date"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/jioinfra_gray"
                />
            <com.jio.jioinfra.custom.TextViewMedium
                android:id="@+id/txt_end_time_ngo"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:text=""
                android:layout_weight="1"
                app:layout_constraintLeft_toLeftOf="parent"
                android:gravity="center"
                android:padding="@dimen/_5sdp"
                android:textSize="@dimen/_8sdp"
                android:textColor="@color/jioinfra_gray"
                />
        </LinearLayout>
        <com.jio.jioinfra.custom.TextViewMedium
            android:id="@+id/txt_hide"
            android:layout_width="@dimen/_50sdp"
            android:paddingRight="@dimen/_5sdp"
            android:layout_height="match_parent"
            android:text="Hide"
            android:layout_weight="1"
            app:layout_constraintTop_toBottomOf="@+id/lll"
            app:layout_constraintRight_toRightOf="parent"
            android:gravity="center"
            android:padding="@dimen/_5sdp"
            android:textSize="@dimen/_8sdp"
            android:textColor="@color/black"
            />

        <View
            android:id="@+id/view_id1"
            android:layout_width="match_parent"
            android:background="@color/black"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@+id/txt_hide"
            android:layout_height="1dp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>