<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <com.jio.jioinfra.custom.TextViewMedium
        android:id="@+id/txt_shift_header"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:layout_marginLeft="@dimen/_10sdp"
        app:layout_constraintTop_toBottomOf="@+id/cnstrntL_calendar_sd"
        app:layout_constraintLeft_toLeftOf="parent"
        android:gravity="left"
        android:padding="@dimen/_2sdp"
        android:textSize="@dimen/_10sdp"
        android:textColor="@color/login_edt_grey"
        />
    <FrameLayout
        android:id="@+id/framid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/txt_shift_header"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        >
        <androidx.cardview.widget.CardView
            android:id="@+id/card_morining_live"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:cardBackgroundColor="@color/white"
            app:cardCornerRadius="@dimen/_10sdp"
            app:cardUseCompatPadding="true"
            android:padding="@dimen/_10sdp"
            android:visibility="visible"
            android:layout_marginLeft="@dimen/_5sdp"
            android:layout_marginRight="@dimen/_5sdp"

            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:id="@+id/llid2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    android:weightSum="6"
                    >
                    <com.jio.jioinfra.custom.TextViewMedium
                        android:id="@+id/txt_resourceAAa"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="RE"
                        android:layout_weight=".3"
                        android:layout_gravity="center"
                        android:visibility="invisible"
                        app:layout_constraintTop_toBottomOf="@+id/txt_date"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:gravity="center"
                        android:padding="@dimen/_5sdp"
                        android:textColor="@color/black"
                        />
                    <com.jio.jioinfra.custom.TextViewMedium
                        android:id="@+id/txt_resource_1"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Resource"
                        android:layout_weight="2.2"
                        android:layout_gravity="center"
                        app:layout_constraintTop_toBottomOf="@+id/txt_date"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:gravity="center|left"
                        android:padding="@dimen/_5sdp"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"
                        />
                    <com.jio.jioinfra.custom.TextViewMedium
                        android:id="@+id/txt_citrix"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Citrix"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        app:layout_constraintTop_toBottomOf="@+id/txt_date"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:gravity="center"
                        android:padding="@dimen/_5sdp"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"
                        />
                    <com.jio.jioinfra.custom.TextViewMedium
                        android:id="@+id/txt_ngo"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="NGO"
                        android:layout_weight="1"
                        app:layout_constraintTop_toBottomOf="@+id/txt_date"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:padding="@dimen/_5sdp"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"
                        />
                    <com.jio.jioinfra.custom.TextViewMedium
                        android:id="@+id/txt_ask"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Alerts Ack"
                        android:layout_weight="1.3"
                        app:layout_constraintTop_toBottomOf="@+id/txt_date"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:gravity="center|left"
                        android:layout_gravity="center"
                        android:padding="@dimen/_5sdp"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"
                        />
                    <com.jio.jioinfra.custom.TextViewMedium
                        android:id="@+id/txt_resourceAAq"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:text="Res"
                        android:layout_weight="1"
                        android:layout_gravity="center"
                        android:visibility="gone"
                        app:layout_constraintTop_toBottomOf="@+id/txt_date"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:gravity="center"
                        android:padding="@dimen/_5sdp"
                        android:textSize="@dimen/_10sdp"
                        android:textColor="@color/black"
                        />

                </LinearLayout>
                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/live_moring_list"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@+id/llid2"
                    ></androidx.recyclerview.widget.RecyclerView>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>


    </FrameLayout>
</androidx.constraintlayout.widget.ConstraintLayout>